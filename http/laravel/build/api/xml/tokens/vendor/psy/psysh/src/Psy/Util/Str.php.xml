  <line no="45">
    <token name="T_DOC_COMMENT">     *</token>
  </line>
  <line no="46">
    <token name="T_DOC_COMMENT">     *      \M-C    Represents character &#x2018;C&#x2019; with the 8th bit set.</token>
  </line>
  <line no="47">
    <token name="T_DOC_COMMENT">     *              Spans characters &#x2018;\241&#x2019; through &#x2018;\376&#x2019;.</token>
  </line>
  <line no="48">
    <token name="T_DOC_COMMENT">     *</token>
  </line>
  <line no="49">
    <token name="T_DOC_COMMENT">     *      \M^C    Represents control character &#x2018;C&#x2019; with the 8th bit set.</token>
  </line>
  <line no="50">
    <token name="T_DOC_COMMENT">     *              Spans characters &#x2018;\200&#x2019; through &#x2018;\237&#x2019;, and &#x2018;\377&#x2019; (as &#x2018;\M^?&#x2019;).</token>
  </line>
  <line no="51">
    <token name="T_DOC_COMMENT">     *</token>
  </line>
  <line no="52">
    <token name="T_DOC_COMMENT">     *      \^C     Represents the control character &#x2018;C&#x2019;.</token>
  </line>
  <line no="53">
    <token name="T_DOC_COMMENT">     *              Spans characters &#x2018;\000&#x2019; through &#x2018;\037&#x2019;, and &#x2018;\177&#x2019; (as &#x2018;\^?&#x2019;).</token>
  </line>
  <line no="54">
    <token name="T_DOC_COMMENT">     *</token>
  </line>
  <line no="55">
