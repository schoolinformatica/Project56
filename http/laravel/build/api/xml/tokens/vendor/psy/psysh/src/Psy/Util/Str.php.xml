    <token name="T_DOC_COMMENT">     *</token>
  </line>
  <line no="42">
    <token name="T_DOC_COMMENT">     *      \040    Represents ASCII space.</token>
  </line>
  <line no="43">
    <token name="T_DOC_COMMENT">     *</token>
  </line>
  <line no="44">
    <token name="T_DOC_COMMENT">     *      \240    Represents Meta-space (&amp;nbsp in HTML).</token>
